#include<bits/stdc++.h>
using namespace std;

string Matriz(string numeros[][100], int n, int m);

int main(){
 int n, m;
 string numeros[100][100];
 
 cout<<"Ingrese un numero del 1 al 100: ";
 cin>>n;
 cout<<"Ingrese un segundo numero del 1 al 100: ";
 cin>>m;

for(int a=0;a<n;a++){
		for(int b=0;b<m;b++){
			cout<<"Digite un numero ["<<a<<"]["<<b<<"]: ";
			cin>>numeros[a][b];
		}
	} 
	cout<<Matriz(numeros,n,m)<<endl;
return 0;
 }
 
 
 string Matriz(string numeros[][100], int n, int m){
    string final="";
	
	for(int a=0;a <n;a++){
		for(int b=0;b<m;b++){
			final+= numeros[a][b]+" " ;
		}
     	final+="\n";
	}
return final;

}
